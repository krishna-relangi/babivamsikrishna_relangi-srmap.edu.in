#include<stdio.h>
  
int main()
{
    int a[30],i,j,k;
    printf("Enter number of elements");
    scanf("%d",&k);
     
    printf("Enter array elements:k");
    for(i=0;i<k;++i)
        scanf("%d",&a[i]);
     
    printf("nEnter element to search:");
    scanf("%d",&j);
     
    for(i=0;i<k;++i)
        if(a[i]==j)
            break;
     
    if(i<k)
        printf("Element found at index %d",i);
    else
        printf("Element not found");
  
    return 0;
}
